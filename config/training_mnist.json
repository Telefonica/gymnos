{
    "problem": {
        "id": "multi-classification_single-label",
        "machine_learning_field": "deep_learning",
        "source": "image"
    },
    "model": {
        "name": "VGG16",
        "id": "vgg16",
        "compilation_options": {
            "optimizer": "rmsprop",
