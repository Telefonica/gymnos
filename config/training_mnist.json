{
    "problem": {
        "id": "multi-classification_single-label",
        "machine_learning_field": "deep_learning",
        "source": "image"
    },
    "model": {
        "name": "VGG16",
        "id": "vgg16",
        "options": {
            "compilation": {
                "optimizer": "rmsprop",
                "loss": "categorical_crossentropy",
                "metrics": ["acc"]
            },
            "custom":{
                "fine-tunning": {
                    "input_width": 48,
                    "input_height": 48,
                    "input_depth": 3,
                    "batch_size": 512,
                    "extra_layers":{
                        "classifier": { 
                            "relu": { 
                                "alpha": 0.1 
                            }
                        }
                    }
                }
            }
        }
    },
    "dataset": {
        "source": "mnist",
        "id": "mnist-digits",
        "type": "images",
        "properties": {
            "image_width": 48,
            "image_height": 48,
            "image_depth": 3,
            "color_mode": "multi",
            "class_mode": "binary",
            "batch_size": 512
        } 
    },
    "session": {
        "num_cores": 6,
        "num_physical_cpus": 1
    },
    "training": {
        "samples": {
            "fit": 50000,
            "validation": 10000,
            "test": 10000
        }
    },
    "hyper_params": {
        "batch_size": 512,
        "epochs": 10,
        "learning_strategy": "supervised"
    }, 
    "artifacts": {
        "tensorboard":{
            "graphs": {
                "default": true,
                "session": true
            },
            "summaries":{
                "scalars": true,
                "histograms": true,
                "images": true
            }
        }
    }
}