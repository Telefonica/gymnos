{
    "dataset": {
        "dataset": {
            "type": "covid19_chest_xray"
        },
        "samples": {
            "train": 0.8,
            "test": 0.2
        },
        "chunk_size": 32,
        "preprocessors": [
            {
                "type": "image_resize",
                "width": 227,
                "height": 227
            },
            {
                "type": "divide",
                "factor": 255.0
            },
            {
                "type": "normalize",
                "mean": [0.485, 0.456, 0.406],
                "std": [0.229, 0.224, 0.225]
            },
            {
                "type": "channels_first"
            }
        ]
    },
    "model": {
        "model": {
            "type": "covid19_chest_xray_cnn",
            "class_weights": [0.72209794, 4.42161339, 0.71995043]
        },
        "training": {
            "epochs": 5
        }
    },
    "tracking": {
        "trackers": [
            {
                "type": "tensorboard"
            }
        ]
    }
}
