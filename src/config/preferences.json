{
    //
    //  Download Manager
    //

    // Path where dataset files are downloaded. Available formats: dataset_name, model_name, uuid, datetime
    "download_dir": "./downloads",
    // Path where compressed files from dataset downloads are extracted. Available formats: dataset_name, model_name, uuid, datetime
    "extract_dir": "./downloads/extracted",
    // Whether or not force downloads if file already exists
    "force_download": false,
    // Whether or not force extraction if file already exists
    "force_extraction": false,

    //
    // HDF5
    //

    // Path where to store optimized HDF5 datasets
    "hdf5_datasets_dir": "./hdf5_datasets",
    // Extension to save HDF5 datasets, the filename will be {dataset_name}.<hdf5_file_extension>
    "hdf5_file_extension": ".h5",
    // Key to store features inside HDF5 files
    "hdf5_features_key": "features",
    // Key to store labels inside HDF5 files
    "hdf5_labels_key": "labels",
    // Key to store info inside HDF5 files
    "hdf5_info_key": "info",
    // Compression to use to save HDF5 dataset, set to null if you don't want compression
    "hdf5_compression": "gzip",
    // Additional compression options. Ignore this parameter if you set "hdf5_compression" to null
    "hdf5_compression_opts": null,

    //
    //  Trainings
    //

    // Directory to store training executions. Available formats: dataset_name, model_name, uuid, datetime
    "executions_dir": "trainings/{dataset_name}/executions",
    // Directory to store tracking artifacts generated from trainings. Available formats: dataset_name, model_name, uuid, datetime
    "trackings_dir": "trainings/{dataset_name}/trackings",
    // The default experiment name if you don't provide one. Available formats: dataset_name, model_name, uuid, datetime
    "default_experiment_name": "{uuid}",

    // Whether or not save trained model to execution directory 
    "save_trained_model": true,
    // Whether or not save trained preprocessors preprocessors to execution directory 
    "save_trained_model_if_errors": false,
    // Whether or not save model
    "save_trained_preprocessors": true,
    // Whether or not save preprocessors to execution directory if errors ocurred
    "save_trained_preprocessors_if_errors": false,
    // Whether or not save training configuration to execution directory
    "save_training_config": true,
    // Whether or not save training configuration to execution directory if errors ocurred
    "save_training_config_if_errors": true,
    // Whether or not save execution results, name, elapsed times, etc ... to execution directory 
    "save_execution_results": true,

    // Directory to save trained model w.r.t execution directory. Only relevant if "save_trained_model" is true
    "trained_model_dir": "artifacts",
    // File path to save pickled trained preprocessors. Only relevant if "save_trained_preprocessors" is true
    "trained_preprocessors_filename": "artifacts/preprocessors.pkl",
    // File path to store training configuration w.r.t execution directory. Only relevant if "save_training_config"
    "training_config_filename": "training_config.json",
    // File path to store execution results w.r.t execution directory. Only relevant if "save_execution_results" is true
    "execution_results_filename": "execution_results.json"
}
