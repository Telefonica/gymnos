{
    "experiment": {
        "name": "Media Tagging Engine",
        "tags": ["mte", "network"]
    },
    "model": {
        "description": "Media Tagging Engine Subscription Classifier",
        "name": "mte",
        "parameters": {
            "input_shape": [7]
        }
    },
    "dataset": {
        "name": "mte",
        "samples": {
            "train": 0.1,
            "test": 0.05
        },
        "preprocessors": [
            {
                "type": "alphanumeric"
            },
            {
                "type": "lemmatization",
                "language": "spanish"
            },
            {
                "type": "tfidf",
                "lowercase": true,
                "language": "spanish",
                "sublinear_tf": true,
                "ngram_range": [1, 2],
                "min_df": 0.5
            },
            {
                "type": "kbest",
                "scorer": "chi2",
                "k": 7
            }
        ]
    },
    "training": {
        "batch_size": 32,
        "epochs": 1
    }
}
