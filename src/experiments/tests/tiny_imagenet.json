{
    "dataset": {
        "name": "tiny_imagenet",
        "one_hot": true,
        "samples": {
            "train": 5,
            "test": 5
        },
        "chunk_size": 32,
        "data_augmentors": [
            {
                "type": "zoom",
                "probability": 0.3,
                "min_factor": 0.5,
                "max_factor": 1.5
            },
            {
                "type": "flip",
                "probability": 0.4,
                "top_bottom_left_right": "LEFT_RIGHT"
            }
        ],
        "preprocessors": [
            {
                "type": "grayscale"
            },
            {
                "type": "image_resize",
                "width": 25,
                "height": 25
            }
        ]
    },
    "model": {
        "name": "keras",
        "parameters": {
            "sequential": [
                {"type": "flatten"},
                {"type": "dense", "units": 200, "activation": "softmax"}
            ],
            "input_shape": [25, 25, 1],
            "loss": "categorical_crossentropy",
            "optimizer": "sgd",
            "metrics": ["acc"]
        }
    }
}
