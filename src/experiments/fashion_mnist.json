{
    "experiment": {
        "name": "Fashion MNIST"
    },
    "model": {
        "name": "fashion_mnist_nn"
    },
    "dataset": {
        "name": "fashion_mnist",
        "samples": {
            "train": 0.8,
            "test": 0.2
        },
        "preprocessors": [
            {
                "type": "divide",
                "factor": 255
            }
        ]
    },
    "training": {
        "batch_size": 64,
        "epochs": 10,
        "validation_split": 0.25
    },
    "tracking": {
        "trackers": [
            {
                "type": "tensorboard"
            },
            {
                "type": "mlflow"
            }
        ]
    }
}
