{
    "experiment": {
        "name": "Fashion MNIST"
    },
    "model": {
        "name": "fashion_mnist_nn",
        "parameters": {
            "input_shape": [28, 28]
        },
        "compilation": {
            "optimizer": "adam",
            "loss": "categorical_crossentropy",
            "metrics": ["acc"]
        }
    },
    "dataset": {
        "name": "fashion_mnist",
        "preprocessors": [
            {
                "type": "divide",
                "factor": 255
            }
        ]
    },
    "training": {
        "samples": {
            "fit": 0.6,
            "val": 0.2,
            "test": 0.2
        },
        "batch_size": 64,
        "epochs": 10
    },
    "tracking": {
        "trackers": [
            {
                "type": "tensorboard"
            },
            {
                "type": "mlflow"
            }
        ]
    }
}
