{
    "experiment": {
        "name": "Dogs vs Cats",
        "tags": ["deep-learning", "vgg16"]
    },
    "model": {
        "description": "Convolutional model for Dogs vs Cats dataset",
        "name": "dogs_vs_cats_cnn",
        "parameters": {
            "input_shape": [120, 120, 3],
            "session": {
                "log_device_placement": true
            }
        }
    },
    "dataset": {
        "name": "dogs_vs_cats",
        "preprocessors": [
            {
                "type": "image_resize",
                "width": 120,
                "height": 120
            },
            {
                "type": "divide",
                "factor": 255
            }
        ]
    },
    "training": {
        "batch_size": 32,
        "epochs": 1,
        "validation_split": 0.25
    },
    "tracking": {
        "trackers": [
            {
                "type": "tensorboard"
            },
            {
                "type": "mlflow"
            }
        ]
    }
}
