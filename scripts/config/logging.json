{
   "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },

    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "standard",
            "stream": "ext://sys.stdout"
        },

        "file": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "standard",
            "filename": "execution.log",
            "encoding": "utf8",
            "mode": "w"
        }
    },

    "loggers": {
        "": {
            "level": "INFO",
            "handlers": ["console", "file"],
            "propagate": "no"
        }
    }
}

